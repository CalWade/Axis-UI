{
  "name": "axis-ui",
  "version": "0.0.3",
  "type": "module",
  "description": "A modern, lightweight Vue 3 component library",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    },
    "./button": {
      "types": "./dist/types/button/index.d.ts",
      "import": "./dist/button/index.js"
    },
    "./checkbox": {
      "types": "./dist/types/checkbox/index.d.ts",
      "import": "./dist/checkbox/index.js"
    },
    "./form": {
      "types": "./dist/types/form/index.d.ts",
      "import": "./dist/form/index.js"
    },
    "./icon": {
      "types": "./dist/types/icon/index.d.ts",
      "import": "./dist/icon/index.js"
    },
    "./input": {
      "types": "./dist/types/input/index.d.ts",
      "import": "./dist/input/index.js"
    },
    "./tree": {
      "types": "./dist/types/tree/index.d.ts",
      "import": "./dist/tree/index.js"
    },
    "./virtual-list": {
      "types": "./dist/types/virtual-list/index.d.ts",
      "import": "./dist/virtual-list/index.js"
    },
    "./resolver": {
      "types": "./dist/types/resolver.d.ts",
      "import": "./dist/resolver.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && vite build --mode umd",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vue3",
    "vue",
    "component-library",
    "axis-ui"
  ],
  "author": "韦贺文",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "async-validator": "^4.2.5",
    "unplugin-vue-define-options": "^3.1.1",
    "@axis-ui/theme-chalk": "workspace:*",
    "@axis-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.1.10",
    "vite-plugin-dts": "^4.5.4"
  }
}
